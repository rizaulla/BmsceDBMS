n","\n","for row in c.execute('SELECT * FROM BRANCH'):\n","  print(row)\n","\n","\n","print('\\n----------------------------------------------\\n')\n","\n","\n","#Bank Account\n","c.execute('''CREATE TABLE IF NOT EXISTS BANK_ACCOUNT (\n","            accno INT PRIMARY KEY,\n","            branch_name VARCHAR(255),\n","            balance DECIMAL(10, 2),\n","            FOREIGN KEY (branch_name) REFERENCES BRANCH (branch_name)\n","        );'''\n",")\n","\n","Bank_Acc=[\n","    (101, 'SBI Branch Kolar', 15000.00),\n","    (102, 'SBI Branch Mangalore', 20000.00),\n","    (103, 'SBI Branch Belgam', 10500.00),\n","    (104, 'SBI Branch Mangalore', 13500.00),\n","    (105, 'SBI Branch WhiteField', 17800.00),\n","    (106, 'SBI Branch Basvangudi', 19800.00)\n","]\n","c.executemany('INSERT INTO BANK_ACCOUNT VALUES(?,?,?)',Bank_Acc)\n","\n","for row in c.execute('Select * from BANK_ACCOUNT'):\n","  print(row)\n","\n","print('\\n----------------------------------------------\\n')\n","\n","#Customer\n","c.execute('''CREATE TABLE IF NOT EXISTS CUSTOMER (\n","    customer_name VARCHAR(255) PRIMARY KEY,\n","    customer_street VARCHAR(255),\n","    customer_city VARCHAR(255)\n",");''')\n","\n","Customer_data = [\n","    ('Mani', 'Dev nagar Road', 'Kolar'),\n","    ('Reddy', 'Basavangudi', 'Mangalore'),\n","    ('Nischay', 'Indiranagar', 'Belgam'),\n","    ('Kumar', 'Ashiok nagar', 'Mangalore'),\n","    ('Zoro', 'Koramangala', 'Bangalore')\n","]\n","\n","c.executemany('Insert INTO CUSTOMER VALUES(?,?,?)', Customer_data)\n","\n","for row in c.execute('Select * from CUSTOMER'):\n","  print(row)\n","\n","\n","print('\\n----------------------------------------------\\n')\n","\n","#Depositor\n","c.execute('''CREATE TABLE IF NOT EXISTS DEPOSITOR (\n","    customer_name VARCHAR(255),\n","    accno INT,\n","    FOREIGN KEY (customer_name) REFERENCES CUSTOMER (customer_name),\n","    FOREIGN KEY (accno) REFERENCES BANK_ACCOUNT (accno));\n","''')\n","\n","Depositor_data=[\n","    ('Mani', 101),\n","    ('Reddy', 102),\n","    ('Nischay', 103),\n","    ('Kumar', 104),\n","    ('Zoro', 105),\n","    ('Zoro',106)\n","]\n","\n","c.executemany('Insert into DEPOSITOR VALUES(?,?)',Depositor_data)\n","\n","for row in c.execute('Select * from DEPOSITOR'):\n","  print(row)\n","\n","\n","print('\\n----------------------------------------------\\n')\n","\n","#Loan\n","c.execute('''CREATE TABLE IF NOT EXISTS LOAN (\n","    loan_number INT PRIMARY KEY,\n","    branch_name VARCHAR(255),\n","    amount DECIMAL(10, 2),\n","    FOREIGN KEY (branch_name) REFERENCES BRANCH (branch_name));\n","''')\n","\n","Loan_data=[\n","    (1, 'SBI Branch Kolar', 58000.00),\n","    (2, 'SBI Branch Mangalore', 28000.00),\n","    (3, 'SBI Branch Belgam', 26050.00),\n","    (4, 'SBI Branch Mangalore', 26550.00),\n","    (5, 'SBI Branch Basvangudi', 45800.00),\n","    (6, 'SBI Branch WhiteField',42000.00)\n","]\n","c.executemany('INSERT into LOAN VALUES(?,?,?)',Loan_data)\n","\n","for row in c.execute('Select * from LOAN'):\n","  print(row)\n","\n","print('\\n----------------------------------------------\\n')\n","\n","#To find customers having more than one account\n","query = '''\n","    SELECT customer_name, COUNT(DISTINCT accno) AS num_accounts\n","    FROM DEPOSITOR\n","    GROUP BY customer_name\n","    HAVING num_accounts > 1;\n","'''\n","\n","print(\"Customers with more than one account:\")\n","for row in c.execute(query):\n","    print(row)\n","\n","print('\\n----------------------------------------------\\n')\n","\n","# To Find Customers having account at all breanches located in a specific city\n","\n","c.execute('''SELECT c.*\n","             FROM CUSTOMER c\n","                  JOIN DEPOSITOR d ON c.customer_name = d.customer_name\n","                  JOIN BANK_ACCOUNT ba ON d.accno = ba.accno\n","                  JOIN BRANCH b ON ba.branch_name = b.branch_name\n","             WHERE b.branch_city = 'Mangalore'\n","             GROUP BY c.customer_name\n","             HAVING COUNT(DISTINCT b.branch_name) = (SELECT COUNT(DISTINCT branch_name)\n","                                                    FROM BRANCH\n","                                                    WHERE branch_city = 'Mangalore'\n",");\n","\n"," ''')\n","print('Account Located in Mangalore are:\\n')\n","for row in c.fetchall():\n","  print(row)\n","\n","print('\\n----------------------------------------------\\n')\n","\n","# To delete all account located in a specific city\n","c.execute('''DELETE FROM BANK_ACCOUNT\n","WHERE branch_name IN (\n","    SELECT branch_name\n","    FROM BRANCH\n","    WHERE branch_city = 'Bangalore'\n",");\n","''')\n","print('All accounts located in Bangalore have been deleted.')\n","for row in c.execute('Select * from BANK_ACCOUNT'):\n","  print(row)\n","\n","print('\\n----------------------------------------------\\n')"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"kOKAIlwCTBDK","executionInfo":{"status":"ok","timestamp":1735194146567,"user_tz":-330,"elapsed":448,"user":{"displayName":"D M Abdul Razzaq","userId":"13446796713828838925"}},"outputId":"a0bec9dd-81ca-4bd9-b37f-bbbbef8d3ca3"},"execution_count":2,"outputs":[{"output_type":"stream","name":"stdout","text":["('SBI Branch Kolar', 'Kolar', 52325.9)\n","('SBI Branch Belgam', 'Belgam', 532462.43)\n","('SBI Branch Mangalore', 'Mangalore', 342342.23)\n","('SBI Branch Basvangudi', 'Bangalore', 834231.22)\n","('SBI Branch WhiteField', 'Bangalore', 734553.26)\n","\n","----------------------------------------------\n","\n","(101, 'SBI Branch Kolar', 15000)\n","(102, 'SBI Branch Mangalore', 20000)\n","(103, 'SBI Branch Belgam', 10500)\n","(104, 'SBI Branch Mangalore', 13500)\n","(105, 'SBI Branch WhiteField', 17800)\n","(106, 'SBI Branch Basvangudi', 19800)\n","\n","----------------------------------------------\n","\n","('Mani', 'Dev nagar Road', 'Kolar')\n","('Reddy', 'Basavangudi', 'Mangalore')\n","('Nischay', 'Indiranagar', 'Belgam')\n","('Kumar', 'Ashiok nagar', 'Man